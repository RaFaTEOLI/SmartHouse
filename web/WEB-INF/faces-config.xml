<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>messages</base-name>
            <var>msgs</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/pessoa/listaPessoas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>incluir</from-outcome>
            <to-view-id>/pessoa/incluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>alterar</from-outcome>
            <to-view-id>/pessoa/alterar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>exc</from-outcome>
            <to-view-id>/pessoa/sucesso.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pessoa/incluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inc</from-outcome>
            <to-view-id>/pessoa/sucesso.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>falha</from-outcome>
            <to-view-id>/pessoa/falha.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pessoa/alterar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>alt</from-outcome>
            <to-view-id>/pessoa/sucesso.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>falha</from-outcome>
            <to-view-id>/pessoa/falha.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/casa/listaCasas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>alterar</from-outcome>
            <to-view-id>/casa/alterar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>incluir</from-outcome>
            <to-view-id>/casa/incluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>exc</from-outcome>
            <to-view-id>/casa/sucesso.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>falha</from-outcome>
            <to-view-id>/casa/falha.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/casa/alterar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>alt</from-outcome>
            <to-view-id>/casa/sucesso.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>falha</from-outcome>
            <to-view-id>/casa/falha.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/casa/incluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inc</from-outcome>
            <to-view-id>/casa/sucesso.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>falha</from-outcome>
            <to-view-id>/casa/falha.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pessoa</from-outcome>
            <to-view-id>/pessoa/listaPessoas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>casa</from-outcome>
            <to-view-id>/casa/listaCasas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
